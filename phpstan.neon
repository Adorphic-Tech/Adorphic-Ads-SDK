parameters:
	level: 0
	paths:
		- .
	excludePaths:
		analyseAndScan:
			- www/devel
			- www/delivery
			- */tests/*
		analyse:
			- lib/vendor
			- lib/pear
			- lib/pclzip
			- lib/simpletest
			- lib/smarty
			- lib/max/language
			- etc
	bootstrapFiles:
		- phpstan-bootstrap.php
	ignoreErrors:
		- '#undefined property#'
		- '#should return#'
